    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/btnView"
        android:visibility="gone"
        android:orientation="vertical"
        >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="755"
           >
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="205"
            android:orientation="horizontal"
            >

            <!-- 왼쪽 공백 부분 시작 -->
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="65">
            </LinearLayout>


            <!--가운데 부분 시작 -->
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="410"
                android:orientation="vertical">


                    <!-- 첫번째 버튼 시작 -->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0px"
                        android:layout_weight="70"
                        android:background="@drawable/information_19"
                        android:orientation="vertical">

                        <!-- 위 공백 -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="15">
                        </LinearLayout>


                        <!-- 버튼부-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="45"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="52">
                            </LinearLayout>
                            <Button
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:background="@drawable/information_10"
                                android:id="@+id/accommodation">
                            </Button>
                            <!--첫번째 버튼 끝-->


                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="49">
                            </LinearLayout>
                            <Button
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:background="@drawable/accommodation_food32"
                                android:id="@+id/bank">
                            </Button>
                            <!-- 두번째 버튼 끝 -->


                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="48">
                            </LinearLayout>
                            <Button
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:background="@drawable/accommodation_food4"
                                android:id="@+id/gasStation">
                            </Button>
                            <!-- 세번째 버튼 끝 -->


                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="49">
                            </LinearLayout>
                            <Button
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:background="@drawable/information_16"
                                android:id="@+id/restaurant">
                            </Button>
                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_height="match_parent"
                                android:layout_weight="52">
                            </LinearLayout>
                            <!-- 네번째 버튼 끝-->

                        </LinearLayout> <!-- 버튼부 끝 -->


                        <!-- 밑 공백 -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="15">
                        </LinearLayout>


                    </LinearLayout> <!-- 이미지 중간부분 끝 -->


                <!-- 두번째 버튼 시작 -->
                <Button
                    android:id="@+id/btn_myDiary"
                    android:layout_width="match_parent"
                    android:layout_height="0px"
                    android:layout_weight="70"
                    android:background="@drawable/information_7">
                </Button>

                <!-- 하단 공백 부분 시작 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0px"
                    android:layout_weight="65">
                </LinearLayout>

             <!-- 가운데 부분 끝 -->
            </LinearLayout>



            <LinearLayout
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="65">
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>